<%= form_with(url: students_path, method: :get, local: true, class: "form-inline") do |form| %>
  <div>
    <%= form.label :major, "Select Major", class: "form-label" %>
    <%= form.select :major, options_for_select(Student::VALID_MAJORS), {prompt: "Any Major" }, name: "search[major]", class: "form-control" %>
  </div>
  <div>
    <%= form.label :graduation_date, "Select Graduation Date Range", class: "form-label" %>
    <div>
    <%= form.date_field :graduation_date_start, name: "search[graduation_date_start]" %>
      <--->
    <%= form.date_field :graduation_date_end, name: "search[graduation_date_end]" %>
    </div>
  </div>
  <div style="padding: 5px">
    <%= form.submit "Search", class: "btn btn-primary" %>
  </div>
<% end %>
